{
  "plugins": [
    "react"
  ],
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "extends": [
    "fbjs",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "no-console": 1,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 1,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "block-scoped-var": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-multi-spaces": 2,
    "no-new-wrappers": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-void": 2,
    "no-warning-comments": 2,
    "no-with": 2,
    "radix": 2,
    "brace-style": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "eol-last": 2,
    "func-names": 2,
    "indent": 0,
    "key-spacing": 2,
    "no-array-constructor": 2,
    "no-continue": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-new-object": 2,
    "no-trailing-spaces": 0,
    "object-curly-spacing": [2, "never"],
    "semi": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": 2,
    "max-len": ["error", { 
      "code": 80,
      "ignoreComments": true,
      "ignoreUrls": true,
      "ignoreStrings": true
    }]
  }
}